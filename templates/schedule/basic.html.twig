{% extends 'base.html.twig' %}
{% block body %}
    <div style="background: white">
        <h1> RJ nr {{schedule.id}} dla poc. {{schedule.service.code}}</h1>
        <ul>
            {% for stop in schedule.stops %}
                <li>Przyj. {{stop.arrivalTime.format("H:i")}}, Odj. {{stop.departureTime.format("H:i")}} |
                    {{stop.station.shortName}}</li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}